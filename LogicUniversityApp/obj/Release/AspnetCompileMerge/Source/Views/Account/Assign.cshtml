
@{
    ViewBag.Title = "Assign";
}

<form action="~/Account/Assign" method="post">
    <input type="hidden" name="userId" value="@ViewBag.id" readonly="readonly" />

    <div class="form-group">
        @Html.Label("user Role", new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.DropDownList("UserRoles", (SelectList)ViewBag.roles, " ")
        </div>
    </div>    
    <input type="submit" class="btn btn-default" value="submit" />
</form>


